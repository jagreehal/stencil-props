{
  "name": "stencil-props",
  "version": "2.0.0",
  "description": "Stencil Props",
  "main": "dist/stencilprops.js",
  "browser": "dist/stencilprops.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "stencil build",
    "dev": "sd concurrent \"stencil build --dev --watch\"",
    "start": "npm run dev",
    "test": "jest --no-cache",
    "test.watch": "jest --watch --no-cache"
  },
  "author": "Jag Reehal",
  "license": "(MIT OR Apache-2.0)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jagreehal/stencil-props.git"
  },
  "dependencies": {
    "@stencil/core": "0.10.4"
  },
  "devDependencies": {
    "@stencil/utils": "^0.0.4",
    "@types/jest": "^23.1.5",
    "jest": "^23.4.0",
    "prettier": "^1.11.1",
    "typescript": "^2.8.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
